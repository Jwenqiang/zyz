// 倒计时分类显示
var filter = {
  dTime: function (value) {
    if(value!=undefined){
      if(value.indexOf(",")>-1){
        return value.split(",");
      }
      return 0; 
    }
  }
}

module.exports = {
  dTime: filter.dTime
}

