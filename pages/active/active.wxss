}

.taskTitle {
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
  text-align: left
}
progress{border:  1px solid #b2e0de;border-radius: 20rpx}
.taskTitle label {
  font-size: 60rpx;
  color: #ff4631;
  font-weight: bold;
}

.taskTitle text {
  color: #ff4631;
}

.lineNum {
  height: 40rpx;
