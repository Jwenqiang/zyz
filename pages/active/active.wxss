.point {
  position: absolute;
  left: 14px;
  overflow: hidden;
  width: 0;
  height: 0;
  border-width: 6px;
  border-style: dashed dashed solid dashed;
  top: -12px;
  border-color: transparent transparent #3dcac3 transparent;
}

.msgTip {
  font-size: 24rpx;
  color: #999;
  margin-top: 20rpx;
  width: 100%;
  text-align: left;
}

.msgTip text {
  font-weight: bold;
  color: #3acbc4;
}

.time {
  /* margin-top: 20rpx; */
  width: 100%;
  position: absolute;
  bottom: 30rpx;
  left: 0
}

.timeT {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
}

.xjDate {
  clear: both;
  font-size: 28rpx;
  color: #999;
  /* width: 430rpx;
  text-align: right; */
}

.xjDate text {
  background: #000;
  padding: 0 8rpx;
