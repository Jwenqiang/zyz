}

.fbtn {
  width: 360rpx;
  float: right;
  padding-top: 10rpx;
}

.fbtn button {
  width: 160rpx;
  height: 80rpx;
  float: right;
  margin-left: 40rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  padding: 0;
  background: -webkit-linear-gradient(left top, #23a09a, #3acac3); /* Safari 5.1 to 6.0 */
  background: -o-linear-gradient(bottom right, #23a09a, #3acac3); /* Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(bottom right, #23a09a, #3acac3); /* Firefox 3.6 to 15 */
  background: linear-gradient(to bottom right, #23a09a, #3acac3); /* 标准语法 */
  border: 0;
  color: #fff;
  font-weight: bold
}
.fbtn button:last-child {
  margin: 0;
  background: -webkit-linear-gradient(left top, #e8542e, #eb8533); /* Safari 5.1 to 6.0 */
  background: -o-linear-gradient(bottom right, #e8542e, #eb8533); /* Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(bottom right, #e8542e, #eb8533); /* Firefox 3.6 to 15 */
  background: linear-gradient(to bottom right, #e8542e, #eb8533); /* 标准语法 */
}

.fbtn button.only {
  width: 300rpx;
  background: -webkit-linear-gradient(left top, #23a09a, #3acac3); /* Safari 5.1 to 6.0 */
  background: -o-linear-gradient(bottom right, #23a09a, #3acac3); /* Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(bottom right, #23a09a, #3acac3); /* Firefox 3.6 to 15 */
  background: linear-gradient(to bottom right, #23a09a, #3acac3); /* 标准语法 */  
}

