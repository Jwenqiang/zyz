/* pages/active/active.wxss */

.page{padding-bottom: 200rpx;background: #df430b}
button::after{
  border: none;
  content: none;
}
.partOne, .partTwo, .partThird, .partFour, .partFive, .partSix {
  padding: 0 40rpx;
  text-align: center;
  width: 100%;
  box-sizing: border-box;
}

.partOne {
  padding: 80rpx 40rpx;
  color: #fff;
  height: 385rpx;
}

.partOne .h1 {
  font-size: 44rpx;
  margin-bottom: 40rpx;
  color: #fff;
  line-height: 1.6;
    line-clamp: 2;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  display: -webkit-box;
  display: box; 
  overflow: hidden;
    -webkit-box-orient: vertical;
    box-orient: vertical;   
}
.partOne .h4{white-space: nowrap;overflow: hidden;text-overflow: ellipsis}

.partTwo {
  height: 730rpx;
  padding-top: 0;
  position: relative;
}

.floatArea {
  position: absolute;
  height: 60rpx;
  padding: 10rpx;
  width: 176rpx;
  left: 40rpx;
  top: -40rpx;
  background: -webkit-linear-gradient(left top, #2d56fa, #4274ef); /* Safari 5.1 to 6.0 */
  background: -o-linear-gradient(bottom right, #2d56fa, #4274ef); /* Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(bottom right, #2d56fa, #4274ef); /* Firefox 3.6 to 15 */
  background: linear-gradient(to bottom right, #2d56fa, #4274ef); /* 标准语法 */
  border-radius: 30px;
  z-index: 8;
  opacity: 0;
  transform: translateX(-200rpx)
}

.floatArea1 {
  position: absolute;
  height: 60rpx;
  padding: 10rpx;
  width: 176rpx;
  left: 40rpx;
  top: -40rpx;
  background: -webkit-linear-gradient(left top, #2d56fa, #4274ef); /* Safari 5.1 to 6.0 */
  background: -o-linear-gradient(bottom right, #2d56fa, #4274ef); /* Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(bottom right, #2d56fa, #4274ef); /* Firefox 3.6 to 15 */
  background: linear-gradient(to bottom right, #2d56fa, #4274ef); /* 标准语法 */
  border-radius: 30px;
  z-index: 8;
  opacity: 1;
  transform: translateX(-200px);
}
.areaMsg {
  width: 100rpx;
  float: left;
  text-align: left;
}

.floatArea image,.floatArea1 image {
  width: 60rpx;
  height: 60rpx;
  float: left;
  border-radius: 50%;
  margin-right: 12rpx;
}

.floatArea label.name,.floatArea1 label.name {
  font-size: 24rpx;
  font-weight: bold;
  color: #ffc700;
  display: block;
  line-height: 1;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis
}

.floatArea .money,.floatArea1 .money {
  font-size: 20rpx;
  color: #fff;
}

.money text {
  color: #ffc700;
  margin-left: 2rpx;
}

.acMsg image {
  height: 700rpx;
  border: 4px solid #f8a88e;
  border-radius: 8px;
  box-sizing: border-box;
}

.partThird {
  height: 690rpx;
  padding: 0rpx 40rpx;
}

.title {
  font-size: 30rpx;
  font-weight: bold;
  color: #fff;
  margin-bottom: 40rpx;
}

.title image {
  width: 75rpx;
  height: 27rpx;
  vertical-align: middle;
  margin-left: 20rpx;
}

.title image:first-child {
  margin: 0 20rpx 0 0;
}

.myTask {
  width: 670rpx;
  height: 562rpx;
  background: #fff;
  border: 4px solid #f8a88e;
  border-radius: 8px;
  box-sizing: border-box;
