.partSix {
  height: 1486rpx;
  margin-top: -1px;
}

.role {
  width: 100%;
  border: 4px solid #f8a88e;
  border-radius: 8px;
  box-sizing: border-box;
  padding: 40rpx;
  background: #fff;
}

.roleMsg {
  line-height: 1.6;
