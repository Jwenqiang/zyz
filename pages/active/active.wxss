}

.footBar image {
  width: 80rpx;
  height: 80rpx;
  display: inline-block;
  float: left;
  border-radius: 50%;
}

.fbName {
  width: 200rpx;
  float: left;
  color: #000;
  height: 100%;
}

.fbName label {
